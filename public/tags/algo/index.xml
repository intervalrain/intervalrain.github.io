&lt;/span&gt;&lt;/span&gt;&lt;span style=&#34;display:flex;&#34;&gt;&lt;span&gt;    cout &lt;span style=&#34;color:#f92672&#34;&gt;&amp;lt;&amp;lt;&lt;/span&gt; sol&lt;span style=&#34;color:#f92672&#34;&gt;-&amp;gt;&lt;/span&gt;trans(strs[&lt;span style=&#34;color:#ae81ff&#34;&gt;1&lt;/span&gt;], res) &lt;span style=&#34;color:#f92672&#34;&gt;&amp;lt;&amp;lt;&lt;/span&gt; endl;
